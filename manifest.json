{
  "manifest_version": 2,
  "name": "vimkeybindings",
  "description": "Vi(m) directional keybindings for Firefox.",
  "author": "Dietrich Ayala",
  "version": "2.0.2",
  "applications": {
    "gecko": {
      "id": "vimkeybindings@autonome.wordpress.com"
    }
  },
  "content_scripts": [
    {   
      "matches":["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_start",
      "match_about_blank": true
    }   
  ]
}
